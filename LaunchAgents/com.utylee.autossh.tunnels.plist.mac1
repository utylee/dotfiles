<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.utylee.autossh.tunnels</string>

  <key>ProgramArguments</key>
  <array>
    <string>/Users/utylee/temp/bin/autossh-tunnels.sh</string>
  </array>

  <key>RunAtLoad</key>
  <true/>

  <key>KeepAlive</key>
  <dict>
    <key>NetworkState</key>
    <true/>
    <key>SuccessfulExit</key>
    <false/>
  </dict>

  <key>ThrottleInterval</key>
  <integer>3</integer>

  <key>StandardOutPath</key>
  <string>/tmp/autossh-tunnels.out</string>
  <key>StandardErrorPath</key>
  <string>/tmp/autossh-tunnels.err</string>
</dict>
</plist>

