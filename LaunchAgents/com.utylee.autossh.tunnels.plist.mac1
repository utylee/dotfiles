<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
  "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.utylee.autossh.tunnels</string>

  <key>ProgramArguments</key>
  <array>
    <string>/opt/homebrew/bin/autossh</string>
    <string>-M</string><string>0</string>
    <string>-N</string>
    <string>-p</string><string>8822</string>

    <string>-o</string><string>ServerAliveInterval=10</string>
    <string>-o</string><string>ServerAliveCountMax=2</string>
    <string>-o</string><string>ExitOnForwardFailure=yes</string>
    <string>-o</string><string>ConnectTimeout=5</string>
    <string>-o</string><string>ConnectionAttempts=1</string>

    <string>-R</string><string>127.0.0.1:13300:localhost:3000</string>
    <!-- <string>-R</string><string>127.0.0.1:2222:localhost:8817</string> -->
    <!-- <string>-R</string><string>127.0.0.1:8817:localhost:8817</string> -->
    <string>-R</string><string>0.0.0.0:8817:localhost:8817</string>

    <string>utylee@192.168.1.202</string>
  </array>

  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <dict>
    <key>NetworkState</key>
    <true/>
  </dict>

  <key>StandardOutPath</key>
  <string>/tmp/autossh-tunnels.out</string>
  <key>StandardErrorPath</key>
  <string>/tmp/autossh-tunnels.err</string>
</dict>
</plist>
