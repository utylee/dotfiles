<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" 
    "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.utylee.mountsmb</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/utylee/temp/bin/mount_smb_shares.sh</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>StartInterval</key>
    <integer>0</integer>

    <key>StandardOutPath</key>
    <string>/tmp/mountsmb.out</string>

    <key>StandardErrorPath</key>
    <string>/tmp/mountsmb.err</string>
</dict>
</plist>
