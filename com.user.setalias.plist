<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.user.setalias</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/set_subnet_alias.sh</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>NetworkState</key>
    <true/>

    <key>KeepAlive</key>
    <false/>

    <key>StandardOutPath</key>
    <string>/tmp/setalias.out</string>

    <key>StandardErrorPath</key>
    <string>/tmp/setalias.err</string>
</dict>
</plist>
